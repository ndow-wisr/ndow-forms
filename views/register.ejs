<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <title>NDOW Forms</title>
    <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet">
    <link rel="stylesheet" href="/stylesheets/main.css">
</head>

<body>

    <% include ./partials/navigation %>
        <div class="container">
            <div class="col-md-6 col-md-offset-3">
                <form class="" action="/register" method="post">
                    <div class="form-group">
                        <label for="first">First Name</label>
                        <input class="form-control" type="text" name="first" placeholder="First Name">
                    </div>
                    <div class="form-group">
                        <label for="last">Last Name</label>
                        <input class="form-control" type="text" name="last" placeholder="Last Name">
                    </div>
                    <div class="form-group">
                        <label for="division">Division</label>
                        <input class="form-control" type="text" name="division" placeholder="Division">
                    </div>
                    <div class="form-group">
                        <label for="username">username</label>
                        <input class="form-control" type="text" name="username" placeholder="username">
                    </div>
                    <div class="form-group">
                        <label for="password">Password</label>
                        <input class="form-control" type="password" name="password" placeholder="password">
                    </div>
                    <button class="btn btn-primary" type="submit" name="signup">Sign Up</button>
                </form>
            </div>

        </div>

</body>

</html>
